          Header="File"
          Loaded="FileMenuOnLoaded" x:ClassModifier="internal"
          xmlns:local="clr-namespace:OC.Assistant.Controls">
    <local:DteSelector x:Name="DteSelector">
        <MenuItem Header="No open TwinCAT project"></MenuItem>
    </local:DteSelector>
